<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.score.dao.TeacherMapper">
    <select id="selectById"  resultType="com.score.bean.Teacher" parameterType="java.lang.Integer">
        select * from teacher where teacherId = #{teacherId,jdbcType=INTEGER}
    </select>
</mapper>