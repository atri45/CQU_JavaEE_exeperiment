<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.score.dao.ClassMapper">
    <select id="getClass"  resultType="com.score.bean.Class" parameterType="java.lang.Integer">
        select * from class where teacherId = #{teacherId,jdbcType=INTEGER}
    </select>
</mapper>
